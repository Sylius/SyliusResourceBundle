<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults />

        <service id="Sylius\Component\Resource\Metadata\Resource\Factory\AttributesResourceMetadataCollectionFactory" />
        <service id="Sylius\Component\Resource\Metadata\Resource\Factory\ResourceMetadataCollectionFactoryInterface" alias="Sylius\Component\Resource\Metadata\Resource\Factory\AttributesResourceMetadataCollectionFactory" />

        <service id="Sylius\Component\Resource\Doctrine\ORM\Metadata\Factory\DoctrineOrmResourceMetadataFactory" decorates="Sylius\Component\Resource\Metadata\Resource\Factory\ResourceMetadataCollectionFactoryInterface">
            <argument type="service" id="sylius.resource_registry" />
            <argument type="service" id="Sylius\Component\Resource\Doctrine\ORM\Metadata\Factory\DoctrineOrmResourceMetadataFactory.inner" />
        </service>

        <service id="Sylius\Component\Resource\Metadata\Operation\Initiator\RequestOperationInitiator">
            <argument type="service" id="sylius.resource_registry" />
            <argument type="service" id="Sylius\Component\Resource\Metadata\Resource\Factory\ResourceMetadataCollectionFactoryInterface" />
        </service>
    </services>
</container>